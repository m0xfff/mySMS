<h1>Students</h1>

<table class="table">
  <tr>
    <th scope="col">Name</th>
    <th scope="col">Email</th>
    <th scope="col">Birth date</th>
  </tr>
  <% @students.each do |student| %>
  <tr>
    <td><%= student.full_name %></td>
    <td><%= student.email %></td>
    <td><%= student.birth_date.to_formatted_s(:long) %></td>
  </tr>
  <% end %>
</table>
<%= will_paginate @students %>
