%h1 🧑‍🎓 Students

%table.table.table-hover
  %thead
    %tr
      %th{scope: "col"} Full Name
      %th{scope: "col"} Email
      %th{scope: "col"} Gender
      %th{scope: "col"} Birth Date
      %th{scope: "col"}
  = render partial: 'shared/links', collection: @students, as: :instance
  = render partial: 'student', collection: @students, as: :student

.d-flex.justify-content-center= will_paginate @students
